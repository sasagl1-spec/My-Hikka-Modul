        }
        self.cooldowns = {}
        self.user_cooldowns = {}
        self.allowed_chats = set()
        
    async def client_ready(self, client, db):
        self._db = db
        self._client = client
        self.settings = self._db.get("SpecificChatRP", "settings", {
            "global_cooldown": 5,
            "user_cooldown": 15,
            "enabled": True,
            "probability": 100  # 100% ÑˆĞ°Ğ½Ñ!
        })
        self.allowed_chats = set(self._db.get("SpecificChatRP", "allowed_chats", []))
    
    async def rpaddchatcmd(self, message: Message):
        """Ğ”Ğ¾Ğ±Ğ°Ğ²Ğ¸Ñ‚ÑŒ Ñ‡Ğ°Ñ‚ Ğ² Ñ€Ğ°Ğ·Ñ€ĞµÑˆĞµĞ½Ğ½Ñ‹Ğµ: .rpaddchat"""
        chat_id = message.chat_id
        if chat_id in self.allowed_chats:
            await message.edit("âœ… Ğ­Ñ‚Ğ¾Ñ‚ Ñ‡Ğ°Ñ‚ ÑƒĞ¶Ğµ Ğ² ÑĞ¿Ğ¸ÑĞºĞµ Ñ€Ğ°Ğ·Ñ€ĞµÑˆĞµĞ½Ğ½Ñ‹Ñ…")
            return
        
        self.allowed_chats.add(chat_id)
        self._db.set("SpecificChatRP", "allowed_chats", list(self.allowed_chats))
        await message.edit("âœ… Ğ§Ğ°Ñ‚ Ğ´Ğ¾Ğ±Ğ°Ğ²Ğ»ĞµĞ½ Ğ² Ñ€Ğ°Ğ·Ñ€ĞµÑˆĞµĞ½Ğ½Ñ‹Ğµ Ğ´Ğ»Ñ Ğ ĞŸ ĞºĞ¾Ğ¼Ğ°Ğ½Ğ´")
    
    async def rpremovechatcmd(self, message: Message):
        """Ğ£Ğ´Ğ°Ğ»Ğ¸Ñ‚ÑŒ Ñ‡Ğ°Ñ‚ Ğ¸Ğ· Ñ€Ğ°Ğ·Ñ€ĞµÑˆĞµĞ½Ğ½Ñ‹Ñ…: .rpremovechat"""
        chat_id = message.chat_id
        if chat_id not in self.allowed_chats:
            await message.edit("âŒ Ğ­Ñ‚Ğ¾Ñ‚ Ñ‡Ğ°Ñ‚ Ğ½Ğµ Ğ² ÑĞ¿Ğ¸ÑĞºĞµ Ñ€Ğ°Ğ·Ñ€ĞµÑˆĞµĞ½Ğ½Ñ‹Ñ…")
            return
        
        self.allowed_chats.remove(chat_id)
        self._db.set("SpecificChatRP", "allowed_chats", list(self.allowed_chats))
        await message.edit("âœ… Ğ§Ğ°Ñ‚ ÑƒĞ´Ğ°Ğ»ĞµĞ½ Ğ¸Ğ· Ñ€Ğ°Ğ·Ñ€ĞµÑˆĞµĞ½Ğ½Ñ‹Ñ…")
    
    async def rplistchatscmd(self, message: Message):
        """Ğ¡Ğ¿Ğ¸ÑĞ¾Ğº Ñ€Ğ°Ğ·Ñ€ĞµÑˆĞµĞ½Ğ½Ñ‹Ñ… Ñ‡Ğ°Ñ‚Ğ¾Ğ²: .rplistchats"""
        if not self.allowed_chats:
            await message.edit("ğŸ“‹ Ğ¡Ğ¿Ğ¸ÑĞ¾Ğº Ñ€Ğ°Ğ·Ñ€ĞµÑˆĞµĞ½Ğ½Ñ‹Ñ… Ñ‡Ğ°Ñ‚Ğ¾Ğ² Ğ¿ÑƒÑÑ‚")
            return
        
        response = "ğŸ“‹ Ğ Ğ°Ğ·Ñ€ĞµÑˆĞµĞ½Ğ½Ñ‹Ğµ Ñ‡Ğ°Ñ‚Ñ‹ Ğ´Ğ»Ñ Ğ ĞŸ:\n\n"
        for chat_id in self.allowed_chats:
            try:
                chat = await self._client.get_entity(chat_id)
                title = getattr(chat, 'title', f'Ğ§Ğ°Ñ‚ {chat_id}')
                response += f"â€¢ {title} (ID: {chat_id})\n"
            except:
                response += f"â€¢ Unknown chat (ID: {chat_id})\n"
        
        await message.edit(response)
    
    async def watcher(self, message: Message):
        """ĞĞ±Ñ€Ğ°Ğ±Ğ¾Ñ‚Ñ‡Ğ¸Ğº Ğ²ÑĞµÑ… ÑĞ¾Ğ¾Ğ±Ñ‰ĞµĞ½Ğ¸Ğ¹"""
        if not self.settings["enabled"] or not message.text or message.out:
            return
        
        # ĞŸÑ€Ğ¾Ğ²ĞµÑ€ÑĞµĞ¼ Ñ€Ğ°Ğ·Ñ€ĞµÑˆĞµĞ½ Ğ»Ğ¸ Ñ‡Ğ°Ñ‚
        chat_id = message.chat_id
        if chat_id not in self.allowed_chats:
            return
        
        current_time = time.time()
        user_id = message.sender_id
        
        # ĞŸÑ€Ğ¾Ğ²ĞµÑ€ĞºĞ° ĞºÑƒĞ»Ğ´Ğ°ÑƒĞ½Ğ¾Ğ²
        if chat_id in self.cooldowns and current_time - self.cooldowns[chat_id] < self.settings["global_cooldown"]:
            return
        
        user_key = f"{chat_id}_{user_id}"
        if user_key in self.user_cooldowns and current_time - self.user_cooldowns[user_key] < self.settings["user_cooldown"]:
            return
        
        # Ğ¨ĞĞĞ¡ 100% - Ğ²ÑĞµĞ³Ğ´Ğ° ÑÑ€Ğ°Ğ±Ğ°Ñ‚Ñ‹Ğ²Ğ°ĞµÑ‚!
        text = message.text.lower()
        
        for pattern, responses in self.triggers.items():
            if re.search(pattern, text):
                response = random.choice(responses)
                
                # Ğ—Ğ°Ğ¼ĞµĞ½Ğ° Ğ¿Ğ»ĞµĞ¹ÑÑ…Ğ¾Ğ»Ğ´ĞµÑ€Ğ¾Ğ²
                if "{sender}" in response and "{target}" in response:
                    sender_name = await self.get_user_name(user_id)
                    target_name = await self.get_user_name((await message.get_reply_message()).sender_id) if message.is_reply else "Ğ²ÑĞµÑ…"
                    response = response.format(sender=sender_name, target=target_name)
                
                await message.reply(response)
                
                self.cooldowns[chat_id] = current_time
                self.user_cooldowns[user_key] = current_time
                break
    
    async def get_user_name(self, user_id: int):
        """ĞŸĞ¾Ğ»ÑƒÑ‡ĞµĞ½Ğ¸Ğµ Ğ¸Ğ¼ĞµĞ½Ğ¸ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»Ñ"""
        try:
            user = await self._client.get_entity(user_id)
            return user.first_name or f"user_{user_id}"
        except:
            return f"user_{user_id}"
    
    async def rptogglecmd(self, message: Message):
        """Ğ’ĞºĞ»ÑÑ‡Ğ¸Ñ‚ÑŒ/Ğ²Ñ‹ĞºĞ»ÑÑ‡Ğ¸Ñ‚ÑŒ Ğ°Ğ²Ñ‚Ğ¾-Ğ ĞŸ: .rptoggle <on/off>"""
        args = utils.get_args_raw(message).lower()
        if args == "on":
            self.settings["enabled"] = True
            await message.edit("âœ… ĞĞ²Ñ‚Ğ¾-Ğ ĞŸ Ñ€ĞµĞ°ĞºÑ†Ğ¸Ğ¸ Ğ²ĞºĞ»ÑÑ‡ĞµĞ½Ñ‹")
        elif args == "off":
            self.settings["enabled"] = False
            await message.edit("âŒ ĞĞ²Ñ‚Ğ¾-Ğ ĞŸ Ñ€ĞµĞ°ĞºÑ†Ğ¸Ğ¸ Ğ²Ñ‹ĞºĞ»ÑÑ‡ĞµĞ½Ñ‹")
        else:
            status = "Ğ²ĞºĞ»ÑÑ‡ĞµĞ½Ñ‹" if self.settings["enabled"] else "Ğ²Ñ‹ĞºĞ»ÑÑ‡ĞµĞ½Ñ‹"
            await message.edit(f"ğŸ“Š ĞĞ²Ñ‚Ğ¾-Ğ ĞŸ: {status}\nĞ˜ÑĞ¿Ğ¾Ğ»ÑŒĞ·ÑƒĞ¹: .rptoggle on/off")
        
        self._db.set("SpecificChatRP", "settings", self.settings)
    
    async def rpstatcmd(self, message: Message):
        """Ğ¡Ñ‚Ğ°Ñ‚Ğ¸ÑÑ‚Ğ¸ĞºĞ° Ğ¼Ğ¾Ğ´ÑƒĞ»Ñ: .rpstat"""
        total_triggers = sum(len(responses) for responses in self.triggers.values())
        await message.edit(
            f"ğŸ“Š Ğ¡Ñ‚Ğ°Ñ‚Ğ¸ÑÑ‚Ğ¸ĞºĞ° Ğ ĞŸ Ğ¼Ğ¾Ğ´ÑƒĞ»Ñ:\n"
            f"â€¢ Ğ’ÑĞµĞ³Ğ¾ Ñ‚Ñ€Ğ¸Ğ³Ğ³ĞµÑ€Ğ¾Ğ²: {total_triggers}\n"
            f"â€¢ Ğ Ğ°Ğ·Ñ€ĞµÑˆĞµĞ½Ğ½Ñ‹Ñ… Ñ‡Ğ°Ñ‚Ğ¾Ğ²: {len(self.allowed_chats)}\n"
            f"â€¢ Ğ’ĞµÑ€Ğ¾ÑÑ‚Ğ½Ğ¾ÑÑ‚ÑŒ: {self.settings['probability']}%\n"
            f"â€¢ Ğ¡Ñ‚Ğ°Ñ‚ÑƒÑ: {'Ğ’ĞšĞ›' if self.settings['enabled'] else 'Ğ’Ğ«ĞšĞ›'}\n"
            f"â€¢ Ğ“Ğ»Ğ¾Ğ±Ğ°Ğ»ÑŒĞ½Ñ‹Ğ¹ CD: {self.settings['global_cooldown']}ÑĞµĞº\n"
            f"â€¢ ĞŸĞ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»ÑŒÑĞºĞ¸Ğ¹ CD: {self.settings['user_cooldown']}ÑĞµĞº"
        )
